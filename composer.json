{
  "name": "bogordesain/backups",
  "description": "Backup database and application.",
  "keywords": [
    "bogordesain",
    "laravel",
    "lumen",
    "backup"
  ],
  "license": "proprietary",
  "require": {
    "php": "^7.2 | ^8.0",
    "ext-zlib": "*",
    "illuminate/console": "~5.7|~6.0|~7.0|~8.0|~9.0|~9.1",
    "illuminate/support": "~5.7|~6.0|~7.0|~8.0|~9.0|~9.1",
    "nesbot/carbon": "~1.36|~2.0|~2.5",
    "spatie/db-dumper": "~2.11||~3.2",
    "symfony/finder": "~4.1|~5.0|~6.0",
    "symfony/process": "~4.1|~5.0|~6.0"
  },
  "require-dev": {
    "orchestra/testbench": "~3.7|~4.0|~5.0|~6.0|~7.0",
    "phpunit/phpunit": "~7.4|~8.0|~9.0"
  },
  "autoload": {
    "psr-4": {
      "Bogordesain\\Backups\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Bogordesain\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Bogordesain\\Backups\\BackupsServiceProvider"
      ]
    }
  }
}
